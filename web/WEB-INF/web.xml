<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<context-param>
	    <description>Nom du serveur MongoDB.</description>
	    <param-name>mongoserver</param-name>
	    <param-value>skymonitor</param-value>
	</context-param>
	
	<context-param>
	    <description>Nom de la base de donnï¿½es MongoDB.</description>
	    <param-name>mongodatabase</param-name>
	    <param-value>db</param-value>
	</context-param>
	
	<listener>
        <description>application startup and shutdown events</description>
        <display-name>ApplicationListener</display-name>
        <listener-class>skymonitor.datacollector.ApplicationListener</listener-class>
	</listener>

</web-app>  
